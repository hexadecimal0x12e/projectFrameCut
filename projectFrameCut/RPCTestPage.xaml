<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="projectFrameCut.RPCTestPage"
             Title="RPCTestPage">
    <VerticalStackLayout>
        <Button x:Name="BootRPC" Text="Boot RPC Server" Clicked="BootRPC_Clicked"/>
        <Button x:Name="LoadDraft" Text="Load Draft" Clicked="LoadDraft_Clicked"/>
        <Entry x:Name="FrameToRender" Placeholder="frame to render" Keyboard="Numeric"/>
        <Button x:Name="GoRender" Text="Go Render" Clicked="GoRender_Clicked"/>
        <Image x:Name="RPCResultImage" Aspect="AspectFit" HeightRequest="300" WidthRequest="300"/> 
    </VerticalStackLayout>
</ContentPage>